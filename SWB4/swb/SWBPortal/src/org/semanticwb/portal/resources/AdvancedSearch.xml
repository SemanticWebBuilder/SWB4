<?xml version="1.0" encoding="ISO-8859-1"?>
<admresource xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <xslfile>admresource.xslt</xslfile>
    <form name="frmAdmSemSearch" action="true" method="POST" enctype="multipart/form-data">
        <fieldset>
            <input type="TEXT" name="prefixFilter" label="Prefijos utilizados (separados por comas):"/>
            <input type="TEXT" name="mapUrl" label="Página del Google map:"/>
            <input type="TEXT" name="detailUrl" label="Página de detalle:"/>
            <input type="TEXT" name="mapKey" label="Llave del API de Google maps:"/>
        </fieldset>
        <fieldset>
            <input type="SUBMIT" name="btnSave" value="Guardar"/>
        </fieldset>
    </form>
</admresource>