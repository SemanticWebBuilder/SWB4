<?xml version="1.0" encoding="ISO-8859-1"?>
<admresource xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<xslfile>admresource.xslt</xslfile>
	<form name="frmAdmin" action="true" method="POST" enctype="multipart/form-data">
        <admdbconnmgr defconn="true"/>
        <fieldset>
            <statictext name="txtLocal">Guardar por:</statictext>
            <input type="RADIO" name="twitterConf" label="Recurso" value="1"/>
            <input type="RADIO" name="twitterConf" label="Usuario" value="2"/>
            <input type="RADIO" name="twitterConf" label="Página" value="3"/>
            <input type="TEXT"  name="noMsg" label="Número de mensajes:" size="2" jsvaltype="js_numbers"/>
        </fieldset>
        <fieldset>
            <input type="SUBMIT" name="btnSave"  value="Guardar"/>
            <input type="RESET"  name="btnReset" value="Cancelar"/>
        </fieldset>
    </form>
</admresource>

