<form htmlType="dojo">
    <fieldset>
    <legend>Grupo de Elementos Jorge</legend>
    <table>
        <th>CLASE INCIDENTE</th>
        <tr>
            <td><label class="swpt:Incidente" prop="title" prueba="" /></td>
            <td><property class="swpt:Incidente" prop="title" prueba1="" formElement="swbxf:TextBox" Mode="Edit" /></td>
        </tr>
        <tr>
            <td><label class="swpt:Incidente" prop="description" prueba="" /></td>
            <td><property class="swpt:Incidente" prop="description" prueba1="" formElement="swbxf:TextArea_Description" Mode="Edit" /></td>
        </tr>
        <!--Clase test-->
        <th>CLASE TEST</th>
        <tr>
            <td><label class="swpt:test" prop="title" prueba="" /></td>
            <td><property class="swpt:test" prop="title" prueba1="" formElement="swbxf:TextArea_Description" Mode="Edit" /></td>
        </tr>
        <tr>
            <td><label class="swpt:test" prop="description" prueba="" /></td>
            <td><property class="swpt:test" prop="description" prueba1="" /></td>
        </tr>
        <!--Clase test2-->
        <th>CLASE TEST2</th>
        <tr>
            <td><label class="swpt:test2" prop="direccion" prueba="" /></td>
            <td><property class="swpt:test2" prop="direccion" prueba1="" Mode="Edit" /></td>
        </tr>
        <tr>
            <td><label class="swpt:test2" prop="longitude" prueba="" /></td>
            <td><property class="swpt:test2" prop="longitude" prueba1="" formElement="swbxf:TextArea_Description" /></td>
        </tr>
    </table>
    </fieldset>
    <fieldset>
    <span align="center">
            <Button type="savebtn"/>
            <Button type="submit" name="accept" title="Concluir Tarea Jorge" isBusyButton="true" />
            <Button isBusyButton="true" name="reject" title="Rechazar Tarea Jorge" type="submit" />     
    </span>
    </fieldset>
</form>
